		"cpp class methods": {
			"prefix": "cpp_class_methods",
			"body": [
			"#include \"$0ClassName.hpp\"",
			"",
			"ClassName::ClassName(void)",
			"{",
			"\tstd::cout << \"ClassName Creator Called\" << std::endl;",
			"}",
			"",
			"ClassName::~ClassName(void)",
			"{",
			"\tstd::cout << \"ClassName Destructor Called\" << std::endl;",
			"}",
			""
			]
		},
		"main cpp": {
			"prefix": "main_cpp",
			"body": [
				"int\tmain(void)",
				"{",
				"\t$0",
				"\treturn 0;",
				"}"
			]
		},
		"print to cout": {
			"prefix": "print_cout",
			"body": "std::cout << $0 << std::endl;"
		}
		
